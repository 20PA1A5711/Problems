//REVERSE OF A LINKED LIST
class Solution {
    public ListNode reverseList(ListNode head) {
        if(head==null){
            return null;
        }

        ListNode prevNode=head;
        ListNode currNode=head.next;
        while(currNode!=null){
            ListNode nextNode=currNode.next;
            currNode.next=prevNode;
            prevNode=currNode;
            currNode=nextNode;
        }
        head.next=null;
        return prevNode;
    }
    
}


//MIDDLE OF THE LINKED LIST
